{
  "name": "mmeddle",
  "version": "0.1.1",
  "description": "a symbolic math workspace for browsers and Node.js",
  "author": "John Fogarty <johnhenryfogarty@gmail.com> (https://github.com/jfogarty)",
  "contributors": [],
  "homepage": "http://jfogarty.org/mmeddle",
  "license": "Apache",
  "repository": {
    "type": "git",
    "url": "https://github.com/jfogarty/mmeddle.git"
  },
  "keywords": [
    "math",
    "mathematics",
    "functions",
    "symbolic",
    "algebra",
    "workspace",
    "units"
  ],
  "dependencies": {
    "commander": "latest",
    "fast-sha256": "latest",
    "lodash": "latest",
    "mathjs": "latest",
    "q": "latest",
    "q-io": "latest",
    "sf": "latest"
  },
  "devDependencies": {
    "async": "latest",
    "coveralls": "latest",
    "del": "latest",
    "gulp": "latest",
    "gulp-bump": "latest",
    "gulp-git": "latest",
    "gulp-jshint": "latest",
    "gulp-minify": "latest",
    "gulp-mocha": "latest",
    "gulp-shell": "latest",
    "gulp-size": "latest",
    "gulp-uglify": "latest",
    "gulp-util": "latest",
    "gulp-webpack": "latest",
    "ink-docstrap": "latest",
    "istanbul": "latest",
    "jsdoc": "latest",
    "jshint-stylish": "latest",
    "mkdirp": "latest",
    "mocha": "latest",
    "mocha-phantomjs": "latest",
    "phantomjs": "latest",
    "run-sequence": "latest",
    "should": "latest",
    "uglify-js": "latest",
    "webpack": "latest"
  },
  "main": "./index",
  "scripts": {
    "docs": "jsdoc ./docs/DEVELOPERS.md -r ./src -d ./api --verbose -t ./node_modules/ink-docstrap/template -c .jsdoc.conf.json",
    "doc2": "jsdoc -r src -d docs/src --verbose -c .jsdoc.conf.json",    
    "coverage": "istanbul cover -x **/external/** ./node_modules/mocha/bin/_mocha -- -u bdd -r should --recursive",
    "watch": "gulp watch",
    "test2": "mocha -u bdd -r should --recursive -R list",
    "testb": "mocha-phantomjs -R list test/testMocha.html",
    "testm": "gulp testm",
    "lint": "gulp lint",
    "test": "gulp",
    "build": "gulp"
  },
  "bin": {
    "mmeddle": "./bin/cli.js"
  },
  "engines": {
    "node": ">= 0.1"
  }
}
