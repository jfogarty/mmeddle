  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  ],  
  "repository": {
    "type": "git",
    "url": "https://github.com/jfogarty/mmeddle.git"
  },
  "bugs": {
    "url": "https://github.com/jfogarty/mmeddle/issues"
  },
  "keywords": [
    "math",
    "mathematics",
    "functions",
    "symbolic",
    "algebra",
    "workspace",
    "units"
  ],
  "dependencies": {
    "ansi": "latest",
    "bignumber.js": "latest",
    "commander": "latest",
    "del": "latest",
    "express": "latest",
    "fast-sha256": "latest",
    "lodash": "latest",
    "mathjs": "latest",
    "mkdirp": "latest",
    "mongodb": "2.0.27",
    "q": "1.2.0",
    "request": "latest",
    "serve-static": "latest",
    "sf": "latest",
    "socket.io": "0.9.17",
    "socket.io-client": "0.9.17",
    "ssh2": "latest"
  },
  "devDependencies": {
    "async": "latest",
    "coveralls": "latest",
    "glob": "latest",
    "grunt": "latest",
    "gulp": "latest",
    "gulp-bump": "latest",
    "gulp-git": "latest",
    "gulp-jshint": "latest",
    "gulp-minify": "latest",
    "gulp-mocha": "latest",
    "gulp-shell": "latest",
    "gulp-size": "latest",
    "gulp-uglify": "latest",
    "gulp-util": "latest",
    "imagemagick": "latest",
    "ink-docstrap": "latest",
    "istanbul": "latest",
    "istanbul-instrumenter-loader": "latest",
    "jsdoc": "latest",
    "jsdom": "latest",
    "jshint-stylish": "latest",
    "jsonlint": "latest",
    "mocha": "latest",
    "mocha-phantomjs": "latest",
    "phantomjs": "1.9.1 - 1.9.7-15",
    "run-sequence": "latest",
    "should": "latest",
    "uglify-js": "latest",
    "webpack": "latest",
    "webpack-stream": "latest"
  },
  "scripts": {
    "cli": "node ./bin/cli.js",
    "docs": "jsdoc ./docs/DEVELOPERS.md -r ./src -d ./api --verbose -t ./node_modules/ink-docstrap/template -c .jsdoc.conf.json",
    "doc2": "jsdoc -r src -d docs/src --verbose -c .jsdoc.conf.json",
    "coverage": "istanbul cover -x **/external/** ./node_modules/mocha/bin/_mocha -- -u bdd -r should --recursive",
    "watch": "gulp watch",
    "test2": "mocha -u bdd -r should --recursive -R list",
    "testb": "mocha-phantomjs -R list test/testMocha.html",
    "testm": "gulp testm",
    "lint": "gulp lint",
    "test": "gulp",
    "build": "gulp",
    "start": "node server.js"
  },
  "bin": {
    "mmeddle": "./bin/cli.js"
  },
  "engines": {
    "node": "0.12.x"
  }
}
