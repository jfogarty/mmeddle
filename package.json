{
  "name": "mmeddle",
  "version": "0.1.5",
  "description": "a symbolic math workspace for browsers and Node.js",
  "author": "John Fogarty <johnhenryfogarty@gmail.com> (https://github.com/jfogarty)",
  "contributors": [],
  "homepage": "http://jfogarty.org/mmeddle",
